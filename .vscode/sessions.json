{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v11/terminal-keeper.json",
    "theme": "tribe",
    "active": "default",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            [
                {
                    "name": "Laravel Serve",
                    "commands": [
                        "php artisan serve"
                    ],
                    "autoExecuteCommands": true,
                    "icon": "server-process",
                    "color": "terminal.ansiGreen"
                }
            ],
            [
                {
                    "name": "Reverb Serve",
                    "commands": [
                        "php artisan reverb:start --debug"
                    ],
                    "autoExecuteCommands": true,
                    "icon": "broadcast",
                    "color": "terminal.ansiBrightMagenta"
                }
            ],
            [
                {
                    "name": "Queue Worker",
                    "commands": [
                        "php artisan queue:work --verbose"
                    ],
                    "autoExecuteCommands": true,
                    "icon": "broadcast",
                    "color": "terminal.ansiBrightWhite"
                }
            ],
            [
                {
                    "name": "Queue Worker (Supervisor)",
                    "commands": [
                        "php artisan queue:work --verbose --supervisor"
                    ],
                    "autoExecuteCommands": false,
                    "icon": "broadcast",
                    "color": "terminal.ansiBrightWhite"
                }
            ],
            [
                {
                    "name": "Vite",
                    "commands": [
                        "npm run dev"
                    ],
                    "autoExecuteCommands": true,
                    "icon": "beaker",
                    "color": "terminal.ansiBlue"
                }
            ],
            [
                {
                    "name": "Artisan",
                    "commands": [
                        ""
                    ],
                    "icon": "bracket-dot",
                    "color": "terminal.ansiRed"
                }
            ],
            [
                {
                    "name": "Npm",
                    "commands": [
                        ""
                    ],
                    "icon": "bracket-dot",
                    "color": "terminal.ansiMagenta"
                }
            ],
            [
                {
                    "name": "Tinker",
                    "commands": [
                        "php artisan tinker"
                    ],
                    "icon": "database",
                    "color": "terminal.ansiYellow"
                }
            ]
        ]
    }
}